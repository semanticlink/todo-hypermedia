{
  "name": "todo-hypermedia-client",
  "version": "0.0.1",
  "author": "toddb",
  "description": "Sample clients to demonstrate the use of hypermedia",
  "productName": "Todo client",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./coverage ./build",
    "dev": "webpack-dev-server --mode development --open --config webpack.dev.js --hot",
    "test": "jest --config=jest.config.js",
    "test-watch": "jest --watch --config=jest.config.js",
    "test-watch-notify": "jest --watch --notify --config=jest.config.js",
    "test-clear-cache": "jest --clearCache",
    "coverage": "jest --coverage --config=jest.config.js",
    "build": "webpack --mode development --config webpack.dev.js --progress --hide-modules",
    "dist-api": "webpack --mode production --config webpack.prod.api.js --colors false --progress false",
    "dist-app": "webpack --mode production --config webpack.prod.app.js --colors false --progress false",
    "publish-api-demo": "yarn run dist-api && aws s3 sync ./dist/api/ s3://cdn.semanticlink.io",
    "publish-app-demo": "yarn run dist-app && aws s3 sync ./dist/app/ s3://todo.semanticlink.io"
  },
  "dependencies": {
    "auth-header": "^1.0.0",
    "auth0-js": "9.10.2",
    "axios": "^0.21.1",
    "bootstrap": "4.3.1",
    "bootstrap-vue": "2.0.0-rc.20",
    "bottleneck": "2.18.1",
    "css-loader": "2.1.1",
    "fast-json-patch": "2.1.0",
    "jquery": "3.4.1",
    "linkifyjs": "^2.1.6",
    "luxon": "1.15.0",
    "popper.js": "^1.15.0",
    "semantic-link": "^0.1.1",
    "underscore": "^1.9.1",
    "vue": "2.6.10",
    "vue-datetime": "^1.0.0-beta.6",
    "vue-ionicons": "3.0.4",
    "vue-localstorage": "^0.6.2",
    "vue-notification": "1.3.16",
    "vue-router": "3.0.6",
    "vue-simple-spinner": "^1.2.8",
    "weekstart": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.4",
    "@types/jest": "24.0.13",
    "@types/jest-each": "0.3.4",
    "@types/mocha": "5.2.6",
    "@types/underscore": "1.8.18",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "2.0.2",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "^5.2.0",
    "eslint": "5.16.0",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-react": "7.13.0",
    "favicons-webpack-plugin": "^0.0.9",
    "html-webpack-plugin": "^3.2.0",
    "jest": "24.8.0",
    "jest-each": "24.8.0",
    "node-sass": "4.12.0",
    "raw-loader": "2.0.0",
    "resolve-url-loader": "3.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "7.3.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.1",
    "typescript": "3.4.5",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.4.1",
    "webpack-merge": "^4.1.4"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "peerDependencies": {
    "css-loader": "^2.1.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.14.7"
  }
}
